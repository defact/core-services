{
  "name": "@defacto/core-services",
  "version": "0.1.0",
  "author": "Johnny Hall",
  "engines": {
    "node": "8.15.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register src/**/*.spec.ts",
    "test:e2e": "mocha -r tsconfig-paths/register -r ts-node/register test/**/*.spec.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate:up": "npm run typeorm migration:run",
    "migrate:down": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@material-ui/icons": "^4.2.1",
    "@nestjs/common": "^5.7.4",
    "@nestjs/core": "^5.7.4",
    "@nestjs/jwt": "^0.3.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.3.0",
    "@sendgrid/mail": "^6.3.1",
    "@types/lodash": "^4.14.134",
    "aws-sdk": "^2.480.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.5",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "faker": "^4.1.0",
    "lodash": "^4.17.11",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "nconf": "^0.10.0",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.0",
    "querystring": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "sql.js": "^0.5.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "typeorm": "^0.2.12",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "testdouble": "^3.10.0",
    "tslint": "5.11.0"
  },
  "volta": {
    "node": "8.15.1"
  }
}
